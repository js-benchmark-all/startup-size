{
  "type": "module",
  "dependencies": {
    "@types/bun": "latest",
    "@types/deno": "latest",
    "mitata": "^1.0.34",
    "nypm": "^0.6.0",
    "picocolors": "^1.1.1",
    "rolldown": "^1.0.0-beta.29",
    "typescript": "^5"
  },
  "scripts": {
    "prep": "bun build.ts",
    "bench:bun": "BUN_JSC_useDFGJIT=0 bun index.ts",
    "bench:node": "node --no-opt --expose-gc index.ts",
    "bench:deno": "deno run --allow-all --v8-flags=--no-opt,--expose-gc index.ts",
    "bench:all": "bun bench:bun && bun bench:node && bun bench:deno"
  }
}
