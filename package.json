{
  "type": "module",
  "dependencies": {
    "@types/bun": "latest",
    "@types/deno": "latest",
    "mitata": "^1.0.34",
    "nypm": "^0.6.0",
    "picocolors": "^1.1.1",
    "rolldown": "^1.0.0-beta.29",
    "typescript": "^5"
  },
  "scripts": {
    "prep": "bun build.ts",
    "clean": "bun clean.ts",
    "bench:bun": "bun index.ts",
    "bench:node": "node --expose-gc index.ts",
    "bench:deno": "deno run --allow-all --v8-flags=--expose-gc index.ts",
    "bench:all": "bun clean && bun bench:bun && bun bench:node && bun bench:deno",
    "bench:all:file": "bun bench:bun > bun.log && bun bench:node > node.log && bun bench:deno > deno.log"
  }
}
